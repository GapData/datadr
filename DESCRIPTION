    'divide.rhDF.R'
    'divJoin.R'
    'divSpecs.R'
    'makeKeys.R'
    'misc.R'
    'print.localDiv.R'
    'print.rhData.R'
    'recombine.R'
    'rhData.R'
    'rhDF.R'
    'updateAttributes.R'
    'divMethods.generic.R'
    'divMethods.local.R'
    'divMethods.rh.R'
    'recApplyMethods.R'
    'recCombineMethods.R'
    'divide.R'
    'moments.R'
    'tabulate.R'
